<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<servlet>
		<servlet-name>SpeedyApply</servlet-name>
		<servlet-class>com.tvi.apply.servlet.Recruit</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SFile</servlet-name>
		<servlet-class>com.tvi.apply.servlet.SFile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SpeedyApply</servlet-name>
		<url-pattern>/apply/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SFile</servlet-name>
		<url-pattern>/file/*</url-pattern>
	</servlet-mapping>
</web-app>
